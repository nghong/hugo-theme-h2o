---
layout: null
---

{{ $list := .Site.Pages }}
{{ $len := (len $list) }}
[
  {{ range $index, $page := $list }}
    {
      "title": "{{ $page.Title }}",
      "tags": "{{ delimit $page.Params.tags ' ' }}",
      "url": "{{ $page.Permalink }}"
    }{{ if ne (add $index 1) $len }},{{ end }}
  {{ end }}
]